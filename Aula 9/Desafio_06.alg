algoritmo "Desafio_06"


var
      num1, num2, divisao : real

inicio
      // entrada de dados
      escreva("Digite um valor : ")
      leia (num1)
      
      escreva("Digite outro valor : ")
      leia (num2)
      
      //processamento
      divisao <- num1 / num2
      
      //saída de dados
      escreval("Resultado =  ", divisao)

 
fimalgoritmo
