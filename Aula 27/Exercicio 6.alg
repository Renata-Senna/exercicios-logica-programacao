algoritmo "Subtração_maior_menor"

var
   num1, num2 : inteiro

inicio
   escreva("Digite um numero: ")
   leia(num1)
   
   escreva("Digite um numero: ")
   leia(num2)
   
   se num1 > num2 entao
      escreval("Diferença: ", (num1-num2) )
   senao
      escreval("Diferença: ", (num2-num1) )
   fimse
   
   

fimalgoritmo
