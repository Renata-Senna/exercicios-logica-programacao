algoritmo "Tabuada"


var

   i, j : inteiro

inicio
      //inicializando
      i <- 1
      
      enquanto i < 11 faca
         para j de 1 ate 10 faca
           escreval(i, " x ", j, " = ", i*j)
         fimpara
         escreval()
         
      //incremento do enquanto
      i <- i + 1
      fimenquanto

fimalgoritmo
