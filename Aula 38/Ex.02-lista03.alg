algoritmo "Ex02_Lista03"

var

   i, soma : inteiro


inicio
       //iterar percorrer o laço
       //inicialização
      i <- 1
       //parada
      enquanto i <= 100 faca
         soma <- soma + i
         escreval("Soma = ", soma)
         //incremento
         i <- i + 1
      fimenquanto
      escreval("Soma final")
      escreval("Soma = ", soma)


fimalgoritmo
