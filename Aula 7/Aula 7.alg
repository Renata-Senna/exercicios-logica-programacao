algoritmo "SOMA2NUMEROS"


var
    A: inteiro
    B, C: inteiro

inicio

      //comando de saída de dados
      escreva("Digite um numero: ")

      //comando de entrada
      leia(A)
      
      escreva("Digite outro numero: ")
      leia(B)
      
      //processamento
      c <- A + B
      
      //fase saída de dados
      escreva (C)
      


fimalgoritmo
