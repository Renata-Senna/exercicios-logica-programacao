algoritmo "idade"

var
   idade : inteiro

inicio
      escreval("Qual a sua idade?")
      leia(idade)
      
      //composta
      
      se(idade >= 67) entao

         escreval("Parabéns, voce já pode ser vacinado!")

      senao
            escreval("Aguarde a sua vez para ser vacinado")

      fimse
      
      escreval("Obrigada por acessar o sistema")
      

fimalgoritmo
