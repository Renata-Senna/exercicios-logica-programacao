algoritmo "Multiplo_10"

var
   //resto da divisão
   num, num2: inteiro

inicio
      escreval("Digite um número: ")
      leia(num)
      escreval("Digite o divisor: ")
      leia(num2)
      
      
      se num % num2 = 0 entao
         escreval(num, " é multiplo de ", num2)
      senao
         escreval(num, " não é multiplo de ", num2)
      fimse
      

fimalgoritmo
